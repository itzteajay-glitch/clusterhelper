{
    "name": "setup",
    "script_path": "conf/scripts/setup.sh",
    "parameters": [
      {
        "name": "git repo",
        "same_arg_param": true,
        "type": "text",
        "default": "git@github.com:itzteajay-glitch/truecharts.git",
        "constant": false,
        "env_var": "YOUR_TRUECHARTS_REPO"
      },
      {
        "name": "gh-ssh-key",
        "type": "text",
        "description": "Please paste your private ssh key here",
        "secure": true,
        "default": "SSH KEY HERE", 
        "pass_as": "env_variable",
        "env_var": "ssh-key"
      }  
    ]
}  